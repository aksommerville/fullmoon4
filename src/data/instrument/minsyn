# minsyn config

#----------------------------------- Instruments ---------------------------------------------

# Program 1 is Dot's violin.
begin 1
  wave 0x80 0x50 0x30 0x10 0x08 0x03
  env 80 0x50 40 0x40 200
end

#--- Tangled Vine ---
begin 2 # lead
  wave 0x50 0 0x20 0 0x08
  env 15 0x40 20 0x20 250 .. 10 0x80 15 0x40 400
end
begin 3 # bass
  wave 0x40
  env 35 0x60 20 0x30 300 .. 20 0xc0 15 0x48 700
  mixwave 0x38 0x20 0x08 0x01
  mixenv 25 0xff 150 0 0
end
begin 4 # oooooh ooh wee ohhh
  wave 0xff
  env 45 0x20 60 0x18 450 .. 40 0x40 50 0x20 800
end
begin 5 # glockenspiel (second verse)
  wave 0x20 0x28 0x30 0x40 0x30 0x20 0x18 0x10
  env 10 0x08 10 0x02 250 .. 10 0x20 15 0x05 500
end
begin 6 # jazz lead
  wave 0xa0 0 0x30 0 0x08
  env 12 0x20 15 0x08 250 .. 8 0x48 10 0x16 400
  mixwave 0x80 0x40 0x22 0x30 0x0a 0x08 0x04 0x01
  mixenv 50 0xff 200 0x40 300 .. 50 0xff 80 0x30 300
end

#--- Seven Circles of a Witch's Soul ---
begin 7 # lead
  wave 0x40 0x28 0x20 0x0c 0x10 0x0a 0x05
  env 40 0x20 40 0x13 400 .. 20 0x60 25 0x28 600
end
begin 8 # bass
  wave 0x40
  env 35 0x60 20 0x30 300 .. 20 0xc0 15 0x48 700
  mixwave 0x38 0x20 0x08 0x01
  mixenv 25 0xff 150 0 0
end
begin 9 # arpeggio and decoration
  wave 0x60 0x30 0x10 0x08 0x04 0x01
  env 20 0x30 20 0x0c 300 .. 10 0x60 10 0x18 500
  mixwave 0xc0 0x10 0x08 0x05 0x02
  mixenv 20 0xff 20 0x80 300 .. 10 0xff 10 0x80 500
end
begin 10 # not actually used; reporting error
  wave 0
  env 0 0 0 0 0
end

#--- Toil and Trouble ---
begin 11 # bass
  wave 0xc0 0 0x30 0 0x08
  env 15 0x10 20 0x08 250 .. 10 0x40 15 0x20 400
  mixwave 0xa0 0x40 0x20 0x10 0x08
  mixenv 80 0xff 100 0xff 100
end
begin 12 # lead
  wave 0xff
  env 15 0x20 20 0x08 250 .. 10 0x60 20 0x1c 400
  mixwave 0x40 0x80 0x30 0x10 0x08
  mixenv 100 0xff 100 0x80 300 .. 50 0xff 50 0xc0 500
end
begin 13 # outtro organ
  wave 0x80 0x20 0x30 0x10 0x08
  env 15 0x10 20 0x08 250 .. 10 0x40 15 0x20 400
end

#--- Eye of Newt ---
