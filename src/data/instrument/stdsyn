# minsyn config

#----------------------------------- Instruments ---------------------------------------------

# Program 1 is Dot's violin.
begin 1
  wave 0x80 0x50 0x30 0x10 0x08 0x03
  env 80 0x50 40 0x40 200
end

#--- Tangled Vine ---
begin 2 # lead
  fm *2.0 2.0 ( 0.125 30 0.25 40 0.125* 200 0.0 .. 0.5 50 1.0 30 0.75* 300 0.0 )
  stdenv ( 15 0.125 20 0.0625 250 .. 10 0.40 15 0.2 400 )
  master 0.500
end
begin 3 # bass
  fm *0.5 3.0 ( 0.25 10 0.25 10 0.25* 200 0.0 .. 1.0 10 1.0 10 1.0* 200 0.0 )
  stdenv ( 30 0.2 40 0.05* 200 .. 15 0.30 40 0.1* 300 )
  master 0.400
end
begin 4 # oooooh ooh wee ohhh
  prefix
    noise 1
  voice
    copy 2 1
    bandpass 2 *1 0.100
    bandpass 2 *1 0.100
    mlt 2 5
    penv 2 ( 200 1.0 50 0.90* 600 )
    add 0 2
  master 0.900
end
begin 5 # glockenspiel (second verse)
  fm *5.71 6
  stdenv ( 15 0.05 20 0.002 300 .. 10 0.10 20 0.02 600 )
  master 0.500
end
begin 6 # jazz lead
  fm *1 3 ( 1.0 40 0.25 50 0.25* 200 0.0 )
  stdenv ( 15 0.15 20 0.02 300 .. 10 0.30 20 0.09 600 )
  master 0.850
end

#--- Seven Circles of a Witch's Soul ---
begin 7 # lead
  fm *2.005 4.0 ( 0.25 30 0.10 10 0.10* 100 0.0 .. 1.0 20 0.40 50 0.30* 300 0.0 )
  stdenv ( 30 0.10 10 0.02* 100 .. 20 0.40 50 0.10* 300 )
end
begin 8 # bass
  wave 0x40
  env 35 0x60 20 0x30 300 .. 20 0xc0 15 0x48 700
  mixwave 0x38 0x20 0x08 0x01
  mixenv 25 0xff 150 0 0
end
begin 9 # arpeggio and decoration
  wave 0x60 0x30 0x10 0x08 0x04 0x01
  env 20 0x30 20 0x0c 300 .. 10 0x60 10 0x18 500
  mixwave 0xc0 0x10 0x08 0x05 0x02
  mixenv 20 0xff 20 0x80 300 .. 10 0xff 10 0x80 500
end
begin 10 # not actually used; reporting error
  wave 0
  env 0 0 0 0 0
end

#--- Toil and Trouble ---
begin 11 # bass
  fm *0.5 2.0 ( 0.0 40 0.3 60 0.2* 100 0.0 .. 0.0 20 1.0 50 0.5* 100 0.0 )
  stdenv ( 10 0.5 10 0.25* 100 )
  master 0.450
end
begin 12 # lead
  fm *2.0 2.0 ( 0.5 50 0.125 10 0.125* 100 0.0 .. 1.0 50 0.25 10 0.25* 300 0.0 )
  stdenv ( 20 0.10 30 0.10* 100 .. 10 0.30 30 0.20* 300 )
  master 0.500
end
begin 13 # outtro organ
  wave 0x80
  env 10 0x80 10 0x40 100
  master 0.850
end

#--- Eye of Newt ---
begin 14 # bass
  fm *0.5 3.0 ( 0.30 30 0.40 10 0.30* 200 0.20 .. 0.10 25 1.0 50 0.5* 200 0.0 )
  stdenv ( 30 0.300 50 0.250* 200 0.0 .. 25 1.0 50 0.25* 200 0.0 )
  master 0.200
end
begin 15 # power chords guitar
  voice
    pfm 1 *1 *1 2 ( 0.5 15 1.0 30 0.75* 500 0.0 )
    penv 1 ( 20 0.5 30 0.3* 200 .. 10 1.0 30 0.3* 500 )
    add 0 1
  suffix
    gain 0 8.0 0.6
  master 0.110
end
begin 16 # lead A (first)
  voice
    pfm 1 *1 *2 2 ( 0.5 10 0.5 40 0.25* 200 0.0 .. 0.5 10 1.0 50 0.50* 500 0.0 )
    penv 1 ( 20 0.6 30 0.200* 100 .. 10 1.0 30 0.250* 600 )
    add 0 1
  master 0.300
end
begin 17 # lead B (plucky)
  voice
    pfm 1 *1 *2 3 ( 0.25 10 0.5 40 0.25* 200 0.0 .. 0.25 10 1.0 50 0.50* 500 0.0 )
    penv 1 ( 20 0.6 30 0.200* 100 .. 10 1.0 30 0.250* 600 )
    add 0 1
  master 0.250
end
begin 18 # lead C (later)
  voice
    pfm 1 *1 *2 3 ( 0.5 10 0.5 40 0.25* 200 0.0 .. 0.5 10 1.0 50 0.50* 500 0.0 )
    penv 1 ( 20 0.6 30 0.200* 100 .. 10 1.0 30 0.250* 600 )
    add 0 1
  suffix
    gain 0 3.0 0.75
  master 0.160
end

#--- Jaws of Wrath ---
begin 19 # bass
  fm *0.5 2.0 ( 0.5 30 1.0 200 0.5* 300 0.0 )
  stdenv ( 30 0.5 30 0.20* 200 .. 15 1.0 30 0.30* 300 )
  master 0.140
end
begin 20 # organ, first verse
  wave 0x80 0 0x40 0 0x20 0 0x08
  env 50 0xff 100 0xc0 300
  master 0.060
end
begin 21 # lead
  prefix 
    osc 1 q4 0xff
  voice
    pfm 2 *1 *0.5 2 b1
    penv 2 ( 30 0.5 20 0.100* 200 .. 15 1.0 20 0.200* 600 )
    add 0 2
  suffix
    gain 0 3.0 0.7
  master 0.125
end
begin 22 # lead, second verse
  fm *2 3.0 ( 0.5 30 0.6 30 0.5* 200 0.0 .. 0.5 15 1.0 30 0.75* 500 0.0 )
  stdenv ( 30 0.5 30 0.2* 200 .. 15 1.0 30 0.4* 500 )
  master 0.200
end

#--- Truffles in Forbidden Sauce ---
begin 23 # basically everything
  prefix
    osc 1 q8 0xff
  voice
    pfm 2 *1 *2 1 b1
    penv 2 ( 25 0.3 25 0.2* 200 .. 10 1.0 25 0.3* 600 )
    add 0 2
  suffix
    delay 0 q1 0.8 0.5 0.5 0.5
  master 0.250
end
begin 24 # single-voice lead in second part
  wave 0xc0 0x20 0x10 0x08 0x04 0x02 0x01
  mixwave 0x40 0x80 0x10 0x08
  env 30 0x10 20 0x04 250 .. 20 0x40 25 0x0c 600
  mixenv 30 0xff 20 0x80 250 .. 20 0xff 25 0x80 600
end
begin 25 # accent chords in first part
  fm *1.5 2
  stdenv ( 40 0.4 20 0.1* 200 .. 20 1.0 20 0.2* 500 )
  master 0.130
end

#--- Seventh Roots of Unity ---
begin 26 # bass
  fm *0.5 1.5 ( 0.4 50 0.6 30 0.5* 200 0.25 .. 0.3 20 1.0 30 0.5* 400 0.25 )
  stdenv ( 50 0.4 30 0.15* 200 .. 20 1.0 30 0.20* 400 )
  master 0.200
end
begin 27 # lead
  voice
    pfm 1 *1 *1 2 ( 1.0 10 0.8 10 0.8* 200 0.3 )
    penv 1 ( 40 0.3 40 0.1* 200 .. 20 1.0 30 0.15* 600 )
    #penv 1 ( 20 1.0 20 0.1 300 )
    add 0 1
  suffix
    delay 0 320 0.8 0.2 0.5 0.6 # absolute interval, because the qnote timing comes out in thirds or something
  master 0.300
end
begin 28 # long-running strings, late entry
  prefix
    osc 1 q3.0 0xff
  voice
    pfm 2 *1 *4 0.5 b1
    penv 2 ( 60 0.9 40 0.8* 400 .. 40 1.0 40 0.8* 700 )
    add 0 2
  master 0.080
end

