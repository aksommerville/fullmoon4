begin 39 # bass
  fm *0.33 5 ( 0.2 50 0.45 50 0.30* 200 0.1 .. 0.1 25 1.0 30 0.45* 400 0.0 )
  stdenv ( 50 0.400 50 0.300* 200 .. 25 1.0 30 0.350* 400 )
