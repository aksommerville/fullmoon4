end

#--- Nearer the Sky ---
begin 39 # lead
  prefix
    noise 2
  voice
    pfm 1 *1 *2 1 ( 0.3 20 0.5 30 0.4* 200 0.2 .. 0.2 15 1.0 30 0.2* 500 0.0 )
    penv 1 ( 20 0.200 30 0.180* 200 .. 15 1.0 30 0.250* 500 )
    add 0 1
    copy 1 2
    bandpass 1 *1 0.100
    bandpass 1 *1 0.100
    mlt 1 10.0
    penv 1 ( 50 0.4 1 0.4* 200 .. 50 1.0 1 1.0* 1000 )
    add 0 1
  master 0.300
end
begin 40 # bass
  fm *0.5 2 ( 0.2 30 0.3 30 0.25* 200 0.2 .. 0.1 20 1.0 40 0.5* 400 0.2 )
  stdenv ( 30 0.4 30 0.3* 200 .. 20 1.0 40 0.3* 400 )
  master 0.160
end
begin 41 # extra chords at end
  fm *1 2
  stdenv ( 30 0.5 40 0.15* 200 .. 20 1.0 40 0.2* 300 )
  master 0.200
end

#--- 42 is special, it's the music teacher's saxophone ---
