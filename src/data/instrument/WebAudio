#--- Blood For Silver ---
begin 33 # lead
  wave ffff
  modRate 1
  modRange 4
  modEnv c0 10 ff 100 80 500 00
  env 20 80 30 30 300 .. 15 ff 30 40 400
end
begin 34 # rhythm guitar
  wave sawtooth
  env 20 50 30 20 300 .. 10 90 50 40 500
end

#--- First Frost ---
begin 35 # lead
  wave ffff b000
  modRate 1
  modRange 1
  modEnv ff 500 80 1000 00 1 00
  env 20 80 20 20 500 .. 10 ff 30 30 1000
end
begin 36 # bass
  wave ffff
  env 40 a0 30 60 400 .. 30 ff 30 80 500
end
begin 37 # late bells
  wave ffff
  modRate 9
  modRange 3
  env 10 a0 25 10 400
end

#--- Gloom For Company ---
begin 38 # lead
  wave 2000 1000
  modRate 2
  modRange 1.5
  env 40 80 40 40 500 .. 30 ff 30 70 1000
end
begin 39 # bass
  wave f0a0 5050
  modRate 0.5
  modRange 1
  env 15 60 20 20 250 .. 10 a0 15 40 400
end

#--- Nearer the Sky ---
begin 39 # lead
  wave c000 2000 1000 0800
  modRate 1
  modRange 1
  modEnv 00 40 ff 100 80 500 80 
  env 20 50 20 18 100 .. 10 c0 15 60 400
end
begin 40 # bass
  wave 8080
  modRate 0.5
  modRange 2.5
  env 20 40 20 10 100 .. 10 60 15 20 400
end
begin 41 # extra chords at end
  wave 4000 0000 2000 0000 1000
  env 20 20 20 08 100 .. 10 50 15 10 400
end

#--- 42 is special, it's the music teacher's saxophone ---
begin 42
  wave 8080 4040 2020 1010
  modRate 0.5
  modEnv 00 20 ff 100 80 100 00
  env 20 80 150 20 100
end

#-------------------------------------- Final Sound Effects --------------------------------

sound BELL
  len 0.600
  fm 0 2000 6.3 ( 5 ) sine
  env 0 ( 0 30 0xffff 50 0x2000 * 0 )
  norm 0 0.200
end

