  env 0 ( 0 100 0xffff * 0 )
  norm 0 1.0
  
  fm 1 ( 100 ) 0.6 ( 4000 ) sine
  env 1 ( 0 100 0 200 0xffff 50 0x2000 * 0 )
  mix 0 1
  
  norm 0 0.200
end

