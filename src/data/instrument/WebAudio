  env 0 ( 0 30 0xffff 400 0 )
  delay 0 0.200 0.5 0.5 0.5 0.5
  env 0 ( 0xffff 1500 0xffff * 0 )
  norm 0 0.500
end

sound UNBURY_TREASURE # Same as dig. Do we want some fanfare?
  len 0.300
  fm 0 ( 300 * 400 ) 0.4 ( 2000 * 3000 ) sine
  env 0 ( 0 30 0x8000 50 0x4000 * 0 )
  fm 1 ( 200 * 600 ) 0.25 ( 1000 80 3000 ) sine
  env 1 ( 0 50 0xffff 80 0x2000 * 0 )
  mix 0 1
  norm 0 0.200
end

