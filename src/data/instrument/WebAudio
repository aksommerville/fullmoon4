sound PUSH
  len 0.400
  noise 0
  bandpass 0 200 100
  env 0 ( 0 60 0xffff 170 0x4000 170 0 )
  
  noise 1
  bandpass 1 400 200
  env 1 ( 0 120 0 60 0xffff 40 0x4000 * 0 )
  mix 0 1
  
  norm 0 0.200
end

